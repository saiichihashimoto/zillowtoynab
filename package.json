{
  "name": "zillowtoynab",
  "version": "0.0.0-development",
  "description": "link a ynab account to a zillow property",
  "keywords": [
    "ynab",
    "you need a budget",
    "zestimate",
    "zillow"
  ],
  "homepage": "https://zillowtoynab.com/",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/zillowtoynab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saiichihashimoto/zillowtoynab.git"
  },
  "bin": {
    "zillow-to-ynab-cron": "lib/cron.js"
  },
  "scripts": {
    "build": "react-node-scripts build $(if [ -n \"$HEROKU\" ]; then echo --no-web; fi)",
    "postbuild": "if [ -z \"$HEROKU\" ]; then react-snap; fi",
    "start": "react-node-scripts start",
    "test": "react-node-scripts test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "stylelint": {
    "extends": "stylelint-config-saiichihashimoto"
  },
  "dependencies": {
    "@feathersjs/authentication": "4.3.0",
    "@feathersjs/authentication-oauth": "4.3.0",
    "@feathersjs/client": "4.3.0",
    "@feathersjs/configuration": "4.3.0",
    "@feathersjs/errors": "4.3.0",
    "@feathersjs/express": "4.3.4",
    "@feathersjs/feathers": "4.3.0",
    "@feathersjs/socketio": "4.3.0",
    "bunyan": "1.8.12",
    "create-react-node-app": "3.6.4",
    "fast-xml-parser": "3.12.20",
    "feathers-memory": "4.0.0",
    "feathers-react-hooks": "1.1.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-is-mounted-hook": "1.0.2",
    "request": "2.88.0",
    "request-promise-native": "1.0.7",
    "throng": "4.0.0"
  },
  "devDependencies": {
    "@qiwi/semantic-release-gh-pages-plugin": "1.10.6",
    "cross-env": "5.2.1",
    "cz-conventional-changelog": "3.0.2",
    "eslint-config-saiichihashimoto": "2.0.37",
    "husky": "3.0.5",
    "lint-my-app": "3.7.15",
    "react-snap": "1.23.0",
    "stylelint-config-saiichihashimoto": "2.0.9"
  },
  "engines": {
    "node": "12.8.1"
  },
  "proxy": "http://localhost:4000"
}
