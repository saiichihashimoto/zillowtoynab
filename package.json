{
  "name": "zillowtoynab",
  "version": "0.0.0-development",
  "description": "link a ynab account to a zillow property",
  "keywords": [
    "ynab",
    "you need a budget",
    "zestimate",
    "zillow"
  ],
  "homepage": "https://zillowtoynab.com/",
  "bugs": {
    "url": "https://github.com/saiichihashimoto/zillowtoynab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saiichihashimoto/zillowtoynab.git"
  },
  "scripts": {
    "build": "react-node-scripts build $(if [ -n \"$HEROKU\" ]; then echo --no-web; fi)",
    "postbuild": "if [ -z \"$HEROKU\" ]; then react-snap; fi",
    "start": "react-node-scripts start",
    "test": "react-node-scripts test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "saiichihashimoto"
  },
  "stylelint": {
    "extends": "stylelint-config-saiichihashimoto"
  },
  "dependencies": {
    "bunyan": "1.8.12",
    "create-react-node-app": "3.1.1",
    "fast-xml-parser": "3.12.19",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.7"
  },
  "devDependencies": {
    "@qiwi/semantic-release-gh-pages-plugin": "1.10.5",
    "cz-conventional-changelog": "3.0.2",
    "eslint-config-saiichihashimoto": "2.0.26",
    "husky": "3.0.3",
    "lint-my-app": "3.7.4",
    "react-snap": "1.23.0",
    "stylelint-config-saiichihashimoto": "2.0.7"
  },
  "engines": {
    "node": "12.6.0"
  }
}
